<fieldset>
  <legend>Security Questions</legend>
  <div class="form-row">
    <div class="form-group">
      <label>
        <input
          type="checkbox"
          name="communicableDisease"
          <%= application.getForm().getSections().security.communicableDisease ? 'checked' : '' %>
          <%= editable === false ? 'disabled' : '' %>
        />
        I have a communicable disease.
      </label>
      <label>
        <input
          type="checkbox"
          name="moneyLaundering"
          <%= application.getForm().getSections().security.moneyLaundering ? 'checked' : '' %>
          <%= editable === false ? 'disabled' : '' %>
        />
        I have been involved in money laundering.
      </label>
      <label>
        <input
          type="checkbox"
          name="drugConspiracy"
          <%= application.getForm().getSections().security.drugConspiracy ? 'checked' : '' %>
          <%= editable === false ? 'disabled' : '' %>
        />
        I have been involved in drug conspiracy.
      </label>
      <label>
        <input
          type="checkbox"
          name="arrestedConvicted"
          <%= application.getForm().getSections().security.arrestedConvicted ? 'checked' : '' %>
          <%= editable === false ? 'disabled' : '' %>
        />
        I have been arrested/convicted before.
      </label>
      <label>
        <input
          type="checkbox"
          name="mentalPhysicalDisorder"
          <%= application.getForm().getSections().security.mentalPhysicalDisorder ? 'checked' : '' %>
          <%= editable === false ? 'disabled' : '' %>
        />
        I have a mental/physical disorder.
      </label>
    </div>
  </div>
</fieldset>
